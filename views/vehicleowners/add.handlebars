<div class="card card-body">
  <h3>Vehicle Owners</h3>
  <form action="/vehicleOwners" method="post">
    <div class="form-group">
      <label for="number">Number</label>
      <input type="text" class="form-control" name="number" required>
    </div>

       <div class="form-group">
      <label for="vehicleId">Vehicle Num</label>
      <input type="text" class="form-control" name="vehicleId" required>
    </div>
         <div class="form-group">
      <label for="customerNum">Customer Num</label>
      <input type="text" class="form-control" name="customerNum" required>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
  </form>
</div>